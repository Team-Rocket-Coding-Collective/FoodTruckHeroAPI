{
  "name": "foodtruck-hero-api",
  "version": "1.0.0",
  "description": "API for Food Truck Hero",
  "main": "server.js",
  "repository": {
    "url": "https://github.com/Team-Rocket-Coding-Collective/FoodTruckHero"
  },
  "scripts": {
    "start": "nodemon ./src/server.js --exec babel-node -e js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-prebuild": "npm install",
    "migrate": "sequelize db:migrate",
    "build": "babel src/ -d lib/",
    "flow": "flow",
    "stop": "pkill --signal SIGINT foodtruck-hero-api"
  },
  "keywords": [
    "Food Truck",
    "Food Truck Hero API"
  ],
  "preinstall": "cp -n src/sequelize/config/config.json src/sequelize/config/config-override.json",
  "author": "psilospore",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "4.15.3",
    "graphql": "^0.8.2",
    "graphql-server-express": "^0.7.2",
    "graphql-tools": "^0.11.0",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "pg": "6.2.3",
    "process-title": "^1.0.0",
    "sequelize": "^3.30.4",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "flow-bin": "^0.47.0",
    "ini": "^1.3.4",
    "nodemon": "^1.11.0",
    "sequelize-cli": "^2.7.0"
  }
}
